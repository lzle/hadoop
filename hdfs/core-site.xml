<!-- 删除的文件保存时间，7天 -->
<property>
<name>fs.trash.interval</name>
<value>10080</value>
<final>false</final>
<source>core-site.xml</source>
</property>

<!-- zkfc与zookeeper会话超时时间，防止频繁切主 -->
<property>
<name>ha.zookeeper.session-timeout.ms</name>
<value>20000</value>
<final>false</final>
<source>core-site.xml</source>
</property
